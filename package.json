{
  "name": "blogger",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev:user-service": "nx run user-service:serve",
    "dev:api-gateway": "nx run api-gateway:serve",
    "dev:api-registry": "nx run api-registry:serve",
    "dev:blogger-service": "nx run blogger:serve",
    "dev:comments-service": "nx run comments-service:serve",
    "dev:votes-service": "nx run votes-service:serve",
    "dev:graphql-service": "nx run graphql-blogger:serve",
    "dev:blogger-frontend": "nx run blogger-frontend:serve",
    "docker:compose": "docker-compose start docker-compose.yml",
    "dev:all": "nx run-many --target=serve --projects=api-registry,user-service,api-gateway,blogger,comments-service,votes-service,graphql-blogger --parallel",
    "build:apps": "nx run-many --target=build --projects=api-registry,user-service,api-gateway,blogger,comments-service,votes-service,graphql-blogger --parallel",
    "build:docker-images": "docker build -f ./apps/api-registry/Dockerfile . -t api-registry && docker build -f ./apps/api-gateway/Dockerfile . -t api-gateway && docker build -f ./apps/user-service/Dockerfile . -t user-service  && docker build -f ./apps/blogger/Dockerfile . -t blogger-service && docker build -f ./apps/comments-service/Dockerfile . -t comments-service && docker build -f ./apps/votes-service/Dockerfile . -t votes-service && docker build -f ./apps/graphql-blogger/Dockerfile . -t graphql-service",
    "db:seed:users": "npx sequelize-cli db:seed --env user --seed 20220908125457-demo-user.js",
    "db:seed:articles": "npx sequelize-cli db:seed --env blogger --seed 20220908125507-demo-articles.js",
    "db:seed:comments": "npx sequelize-cli db:seed --env comments --seed 20220908125517-demo-comments.js",
    "db:seed:votes": "npx sequelize-cli db:seed --env votes --seed 20220908125527-demo-votes.js",
    "generate:api": "./libs/api-client/src/lib/generateApi",
    "test": "nx test"
  },
