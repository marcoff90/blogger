    "@nrwl/jest": "14.6.2",
    "@nrwl/linter": "14.6.2",
    "@nrwl/node": "14.6.2",
    "@nrwl/workspace": "14.6.2",
    "@types/amqplib": "^0.8.2",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
