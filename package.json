    "redis": "^4.3.0",
    "regenerator-runtime": "^0.13.9",
    "run": "^1.4.0",
    "sequelize": "^6.21.4",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "tslib": "^2.3.0",
    "zod": "^3.18.0"
  },
